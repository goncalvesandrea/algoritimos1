programa
{
	
	funcao inicio()
	{
		inteiro num1

		
		escreva("Entre com um número: ")
		leia(num1)

		se (num1 % 5 == 0) {
		    escreva("É divisível por 5")
		}  senao {
		    escreva("Não é divisível por 5")}

	}
}

